{
  "version": 2,
  "name": "nostalgiafy",
  "builds": [
    { "src": "package.json", "use": "@now/next" },
    { "src": "/api/*.ts", "use": "@now/node" }
  ],
  "routes": [{ "src": "/api/(.*)", "dest": "/api/$1" }],
  "env": {
    "SPOTIFY_TOKEN": "@spotify-token",
    "SPOTIFY_SECRET": "@spotify-secret",
    "COOK_KEY": "@serverless-express-cook-key"
  }
}
